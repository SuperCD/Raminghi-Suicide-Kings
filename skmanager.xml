<?xml version="1.0" encoding="utf-8"?>
<install type="component" version="1.5.0">
 <name>SK Manager</name>
 <!-- The following elements are optional and free of formatting constraints -->
 <creationDate>2000-02-22</creationDate>
 <author>Entenwyn</author>
 <!--  The version string is recorded in the components table -->
 <version>1.01</version>
 <!-- The description is optional and defaults to the name -->
 
 <!-- Site Main File Copy Section -->
 <!-- Note the folder attribute: This attribute describes the folder
      to copy FROM in the package to install therefore files copied
      in this section are copied from /site/ in the package -->
 <files folder="site">
  <filename>controller.php</filename>
  <filename>skmanager.php</filename>
  <filename>index.html</filename>
 </files>
 
 <administration>
  <!-- Administration Menu Section -->
  <menu>Suicide Kings</menu>
 
  <!-- Administration Main File Copy Section -->
  <files folder="admin">
  <filename>controller.php</filename>
  <filename>skmanager.php</filename>
  <filename>utility.php</filename>
  <filename>controllers/player.php</filename>
  <filename>models/player.php</filename>
  <filename>views/player/view.html.php</filename>
  <filename>views/player/tmpl/default.php</filename>
  <filename>index.html</filename>
   
  </files>
 
 </administration>
</install>
